{
  "name": "ntwfui",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run generate-types && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint src/",
    "lint:fix": "eslint src/ --fix",
    "generate-types": "node scripts/generate-types.js",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test:e2e:ui": "playwright test --config playwright.config.ts --ui",
    "test:e2e:ui:remote": "npx playwright test --config playwright.config.ts --ui --ui-host=0.0.0.0 --ui-port=8080",
    "test:e2e:debug": "playwright test --config playwright.config.ts --debug"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@xyflow/react": "^12.10.0",
    "ajv": "^8.17.1",
    "astro": "^5.16.11",
    "html-to-image": "^1.11.13",
    "js-yaml": "^4.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@types/bezier-js": "^4.1.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "bezier-js": "^6.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "typescript-eslint": "^8.53.1"
  }
}
